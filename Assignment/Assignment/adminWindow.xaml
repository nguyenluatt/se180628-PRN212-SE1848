<Window x:Class="Assignment.adminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Assignment"
        mc:Ignorable="d"
        Title="adminWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Button Content="Manage Customers" Command="{Binding NavigateToCustomerCommand}" />
            <Button Content="Manage Products" Command="{Binding NavigateToProductCommand}" />
            <Button Content="Manage Orders" Command="{Binding NavigateToOrderCommand}" />
            <Button Content="View Reports" Command="{Binding NavigateToReportCommand}" />
        </StackPanel>
    </Grid>
</Window>
